generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Role {
  ADMIN
  SALES_EXECUTIVE
}

model User {
  id        String @id @default(uuid())
  firstName String
  lastName  String
  email     String @unique
  password  String
  role      Role   @default(SALES_EXECUTIVE)
}
